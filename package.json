  "bugs": {
    "url": "https://github.com/atbulbs/webpack_optimization/issues"
  },
  "homepage": "https://github.com/atbulbs/webpack_optimization#readme",
  "devDependencies": {
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "babel": "^6.23.0",
    "babel-loader": "^8.0.6",
    "cross-env": "^5.2.0",
    "rimraf": "^2.6.3",
    "webpack": "^4.32.2",
    "webpack-cli": "^3.3.2",
    "webpack-dev-server": "^3.4.1",
    "webpack-merge": "^4.2.1"
  }
